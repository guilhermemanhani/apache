<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>br.edu.unifil.servlet.HelloWorldServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>contato</servlet-name>
        <servlet-class>br.edu.unifil.servlet.contatoservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>contato</servlet-name>
        <url-pattern>/hellocontato</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>groups</servlet-name>
        <servlet-class>br.edu.unifil.servlet.groupservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>groups</servlet-name>
        <url-pattern>/groups</url-pattern>
    </servlet-mapping>
</web-app>